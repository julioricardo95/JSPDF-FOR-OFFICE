        <option value="q08">CONSTRUCCION DE UNA IGLESIA O CAPILLA</option>
        <option value="q09">CONSTRUCCION DE UN COMEDOR INFANTIL</option>
        <option value="q10">SERVICIO DE RECOLECCION DE RESIDUOS SOLIDOS O BASURA</option>
        <option value="q11">ESTABLECER RUTAS DE TRANSPORTE COLECTIVO</option>
        
        <!-- Unidades de Salud -->
        <option value="s01">UNIDADES DE SALUD</option>
        <option value="s02">REPARACION DE UNIDADES DE SALUD</option>
        <option value="s03">PERSONAL MEDICO</option>
        <option value="s04">MEDICINAS</option>
        <option value="s05">VACUNAS</option>
        <option value="s06">REDUCIR ENFERMEDADES DIARREICAS</option>
        <option value="s07">MEJORAR LA SALUBRIDAD AMBIENTAL</option>
        <option value="s08">DISMINUIR LA DESNUTRICION INFANTIL</option>
