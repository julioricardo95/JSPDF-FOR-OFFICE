        <!-- Agua Potable -->
        <option value="p01">INSTALACION DEL SERVICIO DE AGUA POTABLE</option>
        <option value="p02">AMPLIAR COBERTURA DEL SERVICIO DE AGUA POTABLE</option>
        <option value="p03">MEJORAR SERVICIO DE AGUA POTABLE</option>
        <option value="p04">MEJORAR LA CALIDAD DEL AGUA POTABLE</option>
