        <!-- Viviendas -->
        <option value="vol">REPARAR DE LA CUBIERTA O TECHO</option>
        <option value="v02">REPARACION DE PAREDES</option>
        <option value="v03">REPARACION DEL PISO</option>
        <option value="v04">AMPLIACION DE VIVIENDA</option>
        <option value="v05">REDUCIR EL HACINAMIENTO</option>
        <option value="v06">VIVIENDA</option>
        <option value="v07">MEJORAMIENTO INTEGRAL DE VIVIENDAS</option>
        
      </select>
  </div>

  <div class="form-group">
      <label for="encuestado" class="form-label">Nombre del Encuestado</label>
      <input type="text" id="encuestado" name="encuestado" class="form-input" placeholder="Nombre del Encuestado" />
